<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	layout="1column"
	xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<script src="Wcb_Catalogslider::js/common.js" async="async" />
	</head>
    <body>
		<referenceContainer name="page.wrapper">
			<container name="content.catslider" label="content top homes" htmlTag="div" htmlClass="content-homes">	
				<block class="Wcb\Catalogslider\Block\Catalogslider" name="prosliderss" template="Wcb_Catalogslider::slider.phtml" />
			</container>
		 </referenceContainer>		
		<move element="prosliderss" destination="page.wrapper" after="main.content"/>
    </body>
</page>