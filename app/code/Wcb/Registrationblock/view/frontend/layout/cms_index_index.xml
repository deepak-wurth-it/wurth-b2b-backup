<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
		<referenceContainer name="page.wrapper">
			<container name="content.regblock" label="content banner homes" htmlTag="div" htmlClass="home-reg">	
				<block class="Wcb\Registrationblock\Block\Registrationblock" name="registrationblock" template="Wcb_Registrationblock::registrationblock.phtml" />
