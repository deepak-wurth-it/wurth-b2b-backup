<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Wcb\NegotiableQuote\Block\RequestQuote" name="custom_list"
                   template="Magento_NegotiableQuote::custom-list.phtml" as='custom_list' before="-"/>
            <referenceBlock name="negotiable_quote_listing" remove="true"/>
        </referenceContainer>
    </body>
</page>
